<script>
	$(function() {
		$("table tbody tr.section:not('.remove')").find("td:first-child").each(function(i){
			str = '<a href="#depth2_'+i+'">'+$(this).text()+'</a>'
			$(".skip-marking").append(str); 
			$(this).closest('tr').attr("id", "depth2_"+i); 
		});
		
		$('.skip-marking a').on('click', function(e){
			e.preventDefault();
			
			var $top = $(this.hash).position().top;
			
			$(this).addClass('on').siblings().removeClass('on');
			$('.table-ia').animate({scrollTop : $top - 30 }, 150);
		})
	}); 
</script>
