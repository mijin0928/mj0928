@charset "utf-8";

/* reset
========================================================================== */
* {box-sizing:border-box; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; -webkit-overflow-scrolling:touch;}

html, body {width:100%; height:100%; margin:0; padding:0;}
body {overflow:visible; min-width:280px; font-size:17px; letter-spacing:0; line-height:1.53; color:#555; font-weight:400; background-color:#fff; font-family:'Noto Sans KR', 'Roboto', -apple-system, BlinkMacSystemFont, 'Helvetica Neue', 'Apple SD Gothic Neo', 'Malgun Gothic', '맑은 고딕', Dotum, '돋움', Arial, sans-serif;}
body.overflow {overflow:hidden;}

.hidden {overflow:hidden; position:absolute; width:1px; height:1px; margin:-1px; border:0; padding:0; clip:rect(0 0 0 0); clip-path:inset(50%); white-space:nowrap; text-indent:-9999px;}

/* 앵커 */
a:link,
a:active {color:#555; text-decoration:none;}
a:visited,
a:hover,
a:focus {color:#555;}
a:hover,
a:focus {color:#3a3a4a; text-decoration:underline;}

/* heading */
h1, h2, h3, h4, h5, h6 {margin:0; padding:0;}

/* etc */
iframe {display:inline-block; border:0;}
figure, figcaption {margin:0; padding:0;}
address {margin:0; padding:0; font-style:normal;}

b, strong {font-weight:700;}
em {font-style:normal;}

/* fieldset */
fieldset {margin:0; padding:0; border:0;}
legend {overflow:hidden; position:absolute; width:1px; height:1px; margin:-1px; padding:0; font-size:1px; line-height:0.1; clip:rect(0 0 0 0); clip-path:inset(50%); white-space:nowrap; text-indent:-9999px;}

/* label */
label {display:inline-block; vertical-align:middle; cursor:pointer;}

/* button */
button {cursor:pointer; border:0; vertical-align:middle;}
button.reset {margin:0; padding:0; background-color:transparent;}

/* placeholder */
::placeholder {color:#717171; opacity:1;}
:-ms-input-placeholder {color:#717171;}
::-ms-input-placeholder {color:#717171;}

@media (max-width: 359px) {
	::placeholder {font-size:14px;}
	:-ms-input-placeholder {font-size:14px;}
	::-ms-input-placeholder {font-size:14px;}
}

/* input */
[type='number']::-webkit-outer-spin-button,
[type='number']::-webkit-inner-spin-button {-webkit-appearance :none; margin:0;}
[type='number'] {-moz-appearance :textfield;}

[type="search"] {-webkit-appearance: none; background-color:transparent;}


/* component
========================================================================== */
/* select */
select {overflow:hidden; white-space:nowrap; text-overflow:ellipsis; height:48px; margin:0; padding:12px 44px 12px 12px; color:#555; font-size:16px; vertical-align:middle; border:1px solid #ccc; border-radius:8px; background:#fff url('../images/common/bg/bg-select-arr.png') no-repeat right 16px center; background-size:16px;
	-webkit-appearance:none;
	-moz-appearance:none;
}
select:hover,
select:focus {outline:0; border-color:#44465b;}
select[disabled] {color:#999; background-color:#f1f1f1; opacity:1;}
select[disabled]:hover {border-color:#ccc;}
select::-ms-expand {display:none;}

/* input :text, password, number, tel, email 등등 */
.input-text {width:100%; height:48px; padding:0 12px; color:#555; border:1px solid #ccc; border-radius:8px; font-size:16px; vertical-align:middle;}
.input-text + .input-text {margin-top:10px;}
.input-text:hover,
.input-text:focus {outline:0; border-color:#44465b;}
.input-text[readonly] {color:#3a3a4a; background-color:#f1f1f1 !important;}
.input-text[disabled] {color:#999; background-color:#f1f1f1 !important;}
.input-text.err {color:#ee0700; border-color:#ee0700;}

/* textarea :width, height 의 경우 다중 클래스로 제어가능 */
.input-textarea {width:100%; height:150px; padding:16px 12px; color:#555; border:1px solid #ccc; border-radius:8px; vertical-align:middle; resize:none;}
.input-textarea:hover,
.input-textarea:focus {outline:0; border-color:#44465b;}
.input-textarea[readonly] {color:#3a3a4a; background-color:#f1f1f1 !important;}
.input-textarea[disabled] {color:#999; background-color:#f1f1f1 !important;}
.input-textarea.err {color:#ee0700; border-color:#ee0700;}

.textarea-limit {position:relative; border:1px solid #ccc; border-radius:8px;}
.textarea-limit .input-textarea {border:0; border-radius:8px 8px 0 0;}
.textarea-limit .limit {margin:0; padding:16px; color:#717171; font-size:14px; line-height:1.1; text-align:right;}
.textarea-limit.hover,
.textarea-limit.focus {border-color:#44465b;}
.textarea-limit.hover .limit,
.textarea-limit.focus .limit {color:#555;}

/* 파일첨부 */
.file-area {display:inline-flex; width:auto; flex-wrap:wrap; position:relative;}
.file-area .text {overflow:hidden; display:inline-block; position:relative; z-index:2; width:100%; height:48px; margin:0; padding:0 16px; color:#555; background-color:#fff; border:1px solid #ccc; border-radius:8px; vertical-align:middle; line-height:46px; cursor:pointer;}
.file-area [type="file"] {overflow:hidden; position:absolute; left:0; top:0; z-index:1; width:100%; height:48px; padding:0; border:0; border-radius:8px; font-size:0;}
.file-area [type="file"]::-ms-value,
.file-area [type="file"]::-ms-browse {border:0; background-color:#fff;}
.file-area label.button {flex-shrink:1; position:relative; z-index:2; margin:0;}
.file-area.txt {flex-wrap:nowrap; width:100%;}
.file-area.txt label.button {flex-shrink:0; margin:0 0 0 10px;}
.file-area.w100pc,
.file-area.w100pc label.button {width:100%;}

/* checkbox, radio */
[type='checkbox'],
[type='radio'] {width:28px; height:28px;}
.bg-chk {display:block; position:relative;}
.bg-chk + .bg-chk {margin-top:10px;}
.bg-chk [type='checkbox'],
.bg-chk [type='radio'] {position:absolute; left:0; top:50%; width:28px; height:28px; margin:-10px 0 0 0; opacity:0;}
.bg-chk label {position:relative; padding:0 0 0 40px;}
.bg-chk label:before {content:''; display:block; position:absolute; left:0; top:50%; width:28px; height:28px; margin:-13px 0 0; cursor:pointer; box-sizing:border-box;}
.bg-chk [type='checkbox'] {position:absolute; left:-99999px;}
.bg-chk [type='checkbox'] + label:before {background:url('../images/common/bg/bg-checkbox.png') no-repeat 0; background-size:28px;}
.bg-chk [type='checkbox']:hover + label:before {background:url('../images/common/bg/bg-checkbox-hover.png') no-repeat 0; background-size:28px;}
.bg-chk [type='checkbox']:checked + label:before {border:0; background:url('../images/common/bg/bg-checkbox-on.png') no-repeat 0; background-size:28px;}
.bg-chk [type='checkbox'][disabled] + label:before {border:0; background:url('../images/common/bg/bg-checkbox-disabled.png') no-repeat 0; background-size:28px;}
.bg-chk [type='checkbox'][disabled]:checked + label:before {background:#ebebeb url('../images/common/bg/bg-checkbox-disabled-on.png') no-repeat 0; background-size:28px;}
.bg-chk [type='checkbox']:focus + label {outline:1px dotted blue;}
.bg-chk [type='radio'] + label:before {background:url('../images/common/bg/bg-radio.png') no-repeat 0; background-size:28px;}
.bg-chk [type='radio']:hover + label:before {background:url('../images/common/bg/bg-radio-hover.png') no-repeat 0; background-size:28px;}
.bg-chk [type='radio']:checked + label:before {background:url('../images/common/bg/bg-radio-on.png') no-repeat 0; background-size:28px;}
.bg-chk [type='radio'][disabled] + label:before {background:url('../images/common/bg/bg-radio-disabled.png') no-repeat 0; background-size:28px;}
.bg-chk [type='radio'][disabled]:checked + label:before {background:url('../images/common/bg/bg-radio-disabled-on.png') no-repeat 0; background-size:28px;}
.bg-chk [type='radio']:focus + label {outline:1px dotted blue;}

/* label-group */
.label-group {margin:-10px 0 0 -30px;}
.label-group .bg-chk {display:inline-block; margin:10px 0 0 30px;}

/* button : 시스템 버튼 */
.button,
a.button {display:inline-flex; justify-content:center; align-items:center; height:48px; padding:0 12px; font-size:16px; color:#3a3a4a; vertical-align:middle; border:1px solid #44465c; text-decoration:none; border-radius:8px; background-color:#fff; transition:opacity 0.3s, color 0.3s, border-color 0.3s, background-color 0.3s;}
.button:hover,
a.button:hover {background-color:#f6f6f6;}
.button[disabled],
.button.disabled,
a.button.disabled {cursor:default; opacity:1; color:#999 !important; border-color:#e1e3e6 !important; background-color:#e1e3e6 !important;}

/* button color */
.button.red {color:#fff; border-color:#ee0700; background-color:#ee0700;}
.button.red:hover {border-color:#d7021b; background-color:#d7021b;}
.button.navy {color:#fff; border-color:#44465c; background-color:#44465c;}
.button.navy:hover {border-color:#333340; background-color:#333340;}
.button.navy:focus {outline-color:#f6312b;}

/* button size */
.button.large {height:56px; padding:15px 0; font-size:18px; line-height:1.06;}

@media (max-width: 359px) {
	.button > em > span {display:block; text-align:center;}
}

/* button-group */
.button-group {display:flex; justify-content:space-between; align-items:center; margin-top:48px;}
.button-group > .button {width:100%; margin:0 8px;}
.button-group > .button:first-child {margin-left:0;}
.button-group > .button:last-child {margin-right:0;}

.button-group.line {margin-top:40px; padding-top:48px; border-top:1px solid #e2e3e7;}

.button-group.column {flex-direction:column;}
.button-group.column .button {margin:16px 0 0;}
.button-group.column .button:first-child {margin-top:0;}

/* button sorting */
.button-sort {position:relative; height:48px; margin:0 8px 0 0; padding:0 12px 0 0; color:#555; background-color:#fff; vertical-align:middle;}
.button-sort:last-child {margin-right:0; padding:0;}
.button-sort:after {content:''; position:absolute; right:0; top:50%; width:1px; height:14px; margin-top:-6px; background-color:#bec2c8;}
.button-sort:last-child:after {display:none;}
.button-sort.on {padding-left:21px; background:url('../images/contents/bg-sort-check.png') no-repeat left center;}

/* button round */
.button.round {border-radius:24px;}

/* button more */
.button.more {width:92px; color:#3a3a4a; border:0; background-color:#f6f6f6;}
.button.more:only-child {margin:0 auto;}

