/* txt */
.bg-txt {box-shadow:inset 0 -13px #f8ef63;}

/* list-dot */
.list-dot {list-style-type:none; margin:0; padding:0;}
.list-dot > li {position:relative; padding-left:10px; margin-top:16px; word-break:keep-all;}
.list-dot > li:first-child {margin:0;}
.list-dot > li:before {content:''; position:absolute; top:13px; left:0; width:4px; height:4px; background-color:#333; border-radius:2px; }
.list-dot > li .tit {margin:0; padding:0; font-weight:500; color:#3a3a4a;}
.list-dot > li .cont {margin-top:10px;}

/* list-dash */
.list-dash {list-style-type:none; margin:0; padding:0;}
.list-dash > li {position:relative; padding-left:10px; word-break:keep-all;}
.list-dash > li:before {content:''; position:absolute; top:11px; left:0; width:4px; height:1px; background-color:#333; }

/* list-decimal */
.list-decimal {margin:0; padding:0;}
.list-decimal > li {margin-left:25px;}

/* table reset */
table {width:100%; margin:0; border:0; table-layout:fixed; border-collapse:collapse; empty-cells:show;}
table caption {overflow:hidden; position:absolute; width:1px; height:1px; margin:-1px; border:0; padding:0; clip:rect(0 0 0 0); clip-path:inset(50%); white-space:nowrap; text-indent:-9999px;}
table th,
table td {padding:0; word-break:break-all;}

/* 게시물 or 데이터 없을 때 */
.no-data {padding:82px 0 28px; text-align:center; border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7; background:url('../images/common/ico/ico-no-data.png') no-repeat center 24px; background-size:48px;}

/* 게시판 - 상단 util */
.box-search .util select {width:100%;}
.box-search .util select + * {margin-top:10px;}
.box-search .util .button {margin-top:10px; width:100%;}
.board-util .sort-area {text-align:right; line-height:1.1;}
.tab-list-full + .board-util {margin-top:40px;}
.tab-list-full + .title-3 {margin-top:40px;}

/* board-list */
.counting {margin:0 0 20px 0; padding-top:48px;}
.board-list {border-top:2px solid #44465b;}
.board-list p {margin:0; padding:0;}

/* board-list ul */
.board-list ul {list-style-type:none; margin:0; padding:0;}
.board-list ul li {padding:24px 0 25px 0; border-bottom:1px solid #e2e3e7; font-size:15px;}
.board-list ul li > a .imp {display:flex; align-items:center;}
.board-list ul li > a .imp p.cate {padding-left:8px; margin:0;}
.board-list ul li > a {position:relative; display:block; text-decoration:none;}
.board-list ul li > a .cate {flex-shrink:0; width:auto; color:#2fa787;}
.board-list ul li > a p {margin:8px 0 0 0;}
.board-list ul li > a .tit {overflow:hidden; position:relative; display:-webkit-box; width:calc(100% - 45px); max-height:57px; line-height:26px; padding:0; font-size:17px; color:#3a3a4a; -webkit-line-clamp:2; -webkit-box-orient: vertical; text-align:left;}
.board-list ul li > a .date {padding:0;}
.board-list ul li > a .ico-list-arr {position:absolute; top:50%; right:0;}

/* board-list table */
.board-list table thead tr th {height:55px; padding:10px; line-height:1.4666; text-align:center; color:#111; font-weight:500; border:1px solid #e2e3e7; background-color:#f4f5f7;}
.board-list table thead tr th:first-child {border-left:0;}
.board-list table thead tr th:last-child {border-right:0;}
.board-list table tr td {height:58px; padding:13px 20px; text-align:center; border:1px solid #e2e3e7;}
.board-list table tr td:first-child {border-left:0;}
.board-list table tr td:last-child {border-right:0;}
.board-list table thead tr:first-child th,
.board-list table tbody tr:first-child th,
.board-list table tbody tr:first-child td {border-top:0;}
.board-list table tr td.link {text-align:left;}
.board-list table tr td.link > a {display:inline-block; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; max-width:100%; vertical-align:middle;}

/* req : 필수 입력 항목 */
.req {overflow:hidden; display:inline-block; width:4px; height:4px; margin:0; font-size:1px; line-height:0.1; text-align:left; text-indent:-9999px; vertical-align:top; border-radius:4px; background-color:#ee0700;}
.req-info {margin:0; padding:0; font-size:15px; color:#717171; text-align:left; line-height:1.1;}

.title-3 + .req-info {margin-top:-10px;}

/* board-form */
.board-form {margin:20px 0 0; padding:24px 0; border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7;}
.board-form > .tit {margin:40px 0 16px 0; font-weight:500; color:#3a3a4a; line-height:1.2;}
.board-form > .tit:first-child {margin-top:0;}
.board-form .txt {margin:0; color:#555;}
.board-form .link-file {display:block; margin-top:10px;}
.board-form .box-agree {margin-top:40px;}
.board-form select {width:100%;}
.board-form select + select {margin-top:10px;}
.board-form .box-flex select + select {margin-top:0;}



.board-form .txt-help {margin-top:10px; margin-bottom:0;}
.board-form .txt-help + .txt-help {margin-top:0;}

/* board-view */
.board-view {padding:24px 0; border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7;}
.board-view .tit-area {position:relative; padding:0 0 24px;}
.board-view .tit-area p {margin:0; padding:0;}
.board-view .tit-area p + p {margin-top:10px;}
.board-view .tit-area .tit {margin:0 0 20px; font-size:22px; line-height:1.36; font-weight:700; color:#3a3a4a;}
.board-view .tit-area .data {margin:16px 0 0 0; font-size:15px; line-height:24px; color:#555;}
.board-view .tit-area .data .people .iconset {margin:-2px 6px 0 0;}
.board-view .tit-area .qr-code {position:absolute; right:40px; top:50%; width:80px; height:80px; margin:-40px 0 0;}
.board-view .tit-area .qr-code img {width:100%; height:100%;}
.board-view .cont-area {padding:24px 0 0 0; border-top:1px solid #e2e3e7;}

/* table-data */
.table-data {border-top:2px solid #44465b;}
.table-data table th,
.table-data table td {border:1px solid #e2e3e7;}
.table-data table th:first-child,
.table-data table td:first-child {border-left:0;}
.table-data table th:last-child,
.table-data table td:last-child {border-right:0;}
.table-data table thead th {height:48px; padding:12px 10px; font-size:15px; color:#111; font-weight:500; background-color:#f4f5f7;}
.table-data table tbody td {height:48px; padding:12px 10px; font-size:16px; text-align:center; background-color:#fff;}

/* table-data 좌우스크롤 */
.table-data.scroll {overflow:hidden; position:relative; z-index:0;}
.table-data.scroll table {width:auto; min-width:100%;}
.table-data.scroll table th,
.table-data.scroll table td {word-break: break-word; white-space:nowrap;}
.table-data.flick-hidden {overflow-x:scroll !important;}

.flick-wrap {position:absolute; z-index:1; width:100%; height:100%; left:0; top:0; background-color:rgba(0, 0, 0, 0.3);}
.flick-wrap .flick-area {width:100%; height:100%; text-align:center;}
.flick-wrap .flick-area span {display:inline-block; position:absolute; left:50%; top:50%; z-index:2; transform:translate(-50%, -50%); width:90%; padding:10px; color:#fff; font-size:14px; text-align:center; border-radius:12px; background-color:#44465c;}

/* 썸네일형 목록 */
.thumbs-list ul {list-style-type:none; overflow:hidden; padding:0;}
.thumbs-list ul li {width:100%; margin-top:48px;}
.thumbs-list ul li:first-child {margin:0;}
.thumbs-list ul li a {display:block;}
.thumbs-list ul li .thumbs-area {overflow:hidden; position:relative; width:100%; padding:0; margin:0; border-radius:24px; border:1px solid #e1e3e6; transition:0.2s;}
.thumbs-list ul li .thumbs-area img {display:block; width:100%; height:100%; transition:all 0.3s linear;}
.thumbs-list ul li .subject {overflow:hidden; margin:16px 0 0; color:#3a3a4a; font-size:17px; font-weight:500; line-height:1.53; white-space:nowrap; text-overflow:ellipsis;}
.thumbs-list ul li .data {margin:16px 0 0;}
.thumbs-list ul li .data span {position:relative; padding:0 0 0 24px; color:#555; font-size:15px;}
.thumbs-list ul li .data span:first-child {padding:0;}
.thumbs-list ul li .data span:first-child:before {display:none;}
.thumbs-list ul li .data span:before {content:''; display:block; position:absolute; top:50%; left:12px; width:1px; height:14px; margin-top:-6px; background-color:#bec2c8;}
.thumbs-list ul li:hover a {text-decoration:none;}

/* 첨부파일 아이콘 */
.add-file-area {margin:0;}
.add-file-area > .tit {margin:0; padding:0; color:#44465c; font-weight:500;}
.add-file-area > .list a,
.add-file-area > .list span {display:block; margin-top:16px; line-height:1.41;}
.add-file-area > .list .iconset {margin-right:8px;}

/* 파일첨부 + 첨부파일 리스트 */
.add-file-area > .list span {position:relative; padding-right:36px;}
.add-file-area > .list span:first-child {margin-top:24px;}
.add-file-area > .list .btn-del {position:absolute; right:0; top:2px; padding:0; width:24px; height:24px; background-image:url('../images/common/ico/ico-delete.png'); background-color:transparent; text-indent:-9999px; background-size:100%;}

/* 이전글, 다음글 */
.prev-next-post {margin-top:48px; border:1px solid #44465b; border-width:1px 0;}
.prev-next-post p {margin:0; padding:0;}
.prev-next-post .post {display:flex; flex-wrap:wrap; align-items:center; padding:25px 0;}
.prev-next-post .post .tit {width:16px; height:16px; padding:0 0 0 17px; text-indent:-9999px;  background:url('../images/common/ico/ico-prev-next-post-arr.png') no-repeat 0 0; background-size:16px;}
.prev-next-post .post .txt {overflow:hidden; width:calc(100% - 17px); white-space:nowrap; text-overflow:ellipsis; padding:0 0 0 8px;}
.prev-next-post .post.next {border-top:1px solid #e2e3e7;}
.prev-next-post .post.next .tit {transform:rotate(180deg);}

/* layer-modal */
.dimed {position:fixed; left:0; right:0; top:0; bottom:0; z-index:3000; background-color:rgba(0,0,0,0);}
.layer-modal {overflow:hidden; display:none; flex-wrap:wrap; justify-content:center; align-items:center; position:fixed; left:0; right:0; top:0; bottom:0; z-index:3001; margin:0; padding:0; opacity:0; transition:opacity 0.15s;}
.layer-modal > .wrap {overflow:hidden; overflow-y:auto; display:block; position:relative; width:100%; height:100%; margin:0; padding:0; background-color:#fff; -ms-overflow-style:-ms-autohiding-scrollbar; scrollbar-width:none;}
.layer-modal > .wrap > .tit-area {overflow:hidden; display:flex; flex-wrap:wrap; align-items:center; position:fixed; left:0; top:0; z-index:999; width:100%; height:60px; padding:0 52px 0 0; border-bottom:1px solid #e1e3e6; background-color:#fff;}
.layer-modal > .wrap > .tit-area .tit {margin:0; padding:5px 0 5px 20px; color:#222; font-weight:500; line-height:1.2;}
.layer-modal > .wrap > .cont-area {padding:90px 20px 30px 20px;}
.layer-modal .btn-close-modal {overflow:hidden; display:block; position:fixed; top:14px; right:10px; z-index:3020; width:32px; height:32px; margin:0; padding:0; border:0; font-size:1px; line-height:1px; text-indent:-9999px; background:url('../images/common/btn/btn-close-modal.png') no-repeat 0 0; background-size:100%;}
.layer-modal.over > .wrap:after {content:""; display:block; position:absolute; left:0; top:0; right:0; bottom:0; z-index:3002; background-color:rgba(0,0,0,0.5);}
.layer-modal > .wrap > .cont {padding:80px 20px 20px;}

.layer-modal.on {opacity:1;}

/* 회색 둥근 border box */
.box-gray {padding:20px; border:1px solid #e1e3e6; border-radius:12px;}
.box-gray + .box-gray {margin-top:24px;}
.box-gray + .title-3 {margin:48px 0 20px;}

/* 회색 둥근 bg box */
.box-gray-bg {padding:20px; background-color:#f7f9fa; border-radius:12px;}
.box-gray-bg + .req-info {margin-top:24px;}

/* 이용동의 박스 */
.box-agree {overflow:hidden; border:1px solid #e2e3e7; border-radius:12px;}
.box-agree .terms {overflow-y:scroll; max-height:200px; padding:24px 20px;}
.box-agree .terms .tit-01 {margin:0 0 10px; font-weight:700; color:#3a3a4a; line-height:1.65;}
.box-agree .terms ol,
.box-agree .terms ul {margin:0; padding-left:1.2em;}
.box-agree .agree {padding:24px 20px;border-top:1px solid #e2e3e7;}

.box-line {border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7;}
.box-line .terms-agree {margin:24px 0 40px 0;}

/* 토글기능 박스 */
.box-toggle {overflow:hidden; margin:0; padding:0; border-radius:12px; border:1px solid #e1e3e6;}
.box-toggle .tit-area {overflow:hidden; position:relative; padding:0; border-radius:12px 12px 0 0;}
.box-toggle .tit-area .tit {display:block; width:100%; padding:16px 64px 16px 76px; text-align:left; font-weight:500; line-height:1.41; color:#3a3a4a; background-color:#fff6f2;}
.box-toggle .tit-area .tit .tagset {position:absolute; left:20px; top:50%; margin-top:-11px;}
.box-toggle .tit-area .tit .ico-toggle-arr {position:absolute; right:20px; top:50%; margin-top:-12px;}
.box-toggle .cont-area {padding:24px 20px;}
.box-toggle + .box-toggle {margin-top:16px;}
.box-toggle.off .tit-area .tit {background-color:#f4f5f7;}

.box-toggle.default .tit-area .tit {padding-left:20px;}

.box-toggle .cont-area .survey-select select {width:100%;}
.box-toggle .cont-area .survey-select select + select {margin-top:10px;}
.box-toggle .cont-area .board-form {margin:0; padding:0; border:0;}
.box-toggle .cont-area .board-view {margin:0; padding:0; border:0;}

/* 기타 항목 박스 */
.etc-chk-input {margin-top:10px;}
.etc-chk-input .bg-chk {flex-shrink:0; display:block; width:100px; margin:0 0 10px;}

.box-input-button {display:inline-flex; align-items:center; width:100%;}
.box-input-button .input-text {display:block; margin:0;}
.box-input-button .input-text + *,
.box-input-button .wrap-limit + * {flex-shrink:0; margin:0 0 0 10px;}
.box-input-button .button-group .button {width:auto; margin:0;}
.box-input-button .button-group .button + .button {margin:0 0 0 6px;}

.box-flex {display:flex;}
.box-flex > * {width:100%; margin:0 5px; flex-grow:1;}
.box-flex > *:first-child {margin-left:0;}
.box-flex > *:last-child {margin-right:0;}

/* tab-list */
.tab-list {display:block; list-style-type:none; margin:-10px 0 24px 0; padding:0;}
.tab-list > li {display:inline-block; margin:10px 0 0 0; padding:0; vertical-align:top;}
.tab-list > li > a,
.tab-list > li > button {overflow:hidden; display:block; height:34px; padding:0 16px; line-height:32px; font-size:16px; color:#3a3a4a; border:1px solid #e1e3e6; border-radius:24px; text-decoration:none; background-color:#fff; transition:background-color 0.2s;}
.tab-list > li > a:hover,
.tab-list > li > button:hover,
.tab-list > li.on > a,
.tab-list > li.on > button {color:#fff; background-color:#44465b;}
.tab-list > li.on > a:focus,
.tab-list > li.on > button:focus {outline-color:#f6312b;}
.tab-list > li:last-child {margin-right:0;}

/* tab-list-line */
.tab-list-line {overflow:hidden; position:relative; margin:0 -20px 24px; padding:0 20px;}
.tab-list-line .scroll {overflow:hidden; overflow-x:scroll; white-space:nowrap; -ms-overflow-style:-ms-autohiding-scrollbar; scrollbar-width:none;}
.tab-list-line .scroll::-webkit-scrollbar {display:none;}
.tab-list-line .scroll .tab-list {display:inline-block; margin-bottom:0; vertical-align:top;}
.tab-list-line .list {list-style-type:none; margin:0; padding:0;}
.tab-list-line .list:after {content:''; position:absolute; bottom:0; left:0; display:block; width:1000px; height:1px; background-color:#e1e3e6;}
.tab-list-line .list > li {display:inline-block; padding:0 0 0 20px; vertical-align:top;}
.tab-list-line .list > li > a,
.tab-list-line .list > li > button {position:relative; overflow:hidden; display:block; padding:0; line-height:48px; font-size:16px; color:#717171; background-color:#fff;}
.tab-list-line .list > li > a:hover,
.tab-list-line .list > li > button:hover,
.tab-list-line .list > li.on > a,
.tab-list-line .list > li.on > button {font-weight:500; color:#3a3a4a;}
.tab-list-line .list > li.on > a:after,
.tab-list-line .list > li.on > button:after {content:''; position:absolute; bottom:0; left:0; display:block; width:100%; height:3px; background-color:#ee0700; z-index:1;}
.tab-list-line .list > li.on > a:focus,
.tab-list-line .list > li.on > button:focus {outline-color:#f6312b;}
.tab-list-line .list > li:first-child {padding:0;}

/* tab 가로 스크롤 */
.hr-scroll {position:relative; margin:24px 0; padding:0 34px 0 0;}
.hr-scroll::after {content:''; display:block; position: absolute; right:34px; top:0; width:26px; height:34px; background-image: linear-gradient(to right, rgba(255, 255, 255, 0) 10%, rgba(255, 255, 255, 0.3) 30%, rgba(255, 255, 255, 1) 60%);}
.hr-scroll .scroll {overflow:hidden; overflow-x:scroll; white-space:nowrap; -ms-overflow-style:-ms-autohiding-scrollbar; scrollbar-width:none;}
.hr-scroll .scroll::-webkit-scrollbar {display:none;}
.hr-scroll .scroll .tab-list {display:inline-block; margin-bottom:0; vertical-align:top;}
.hr-scroll .btn-hr-toggle {display:block; position:absolute; right:0; top:0; z-index:2; width:34px; height:34px; margin:0; padding:0; border:0; background-color:transparent;}
.hr-scroll .btn-hr-toggle .iconset {display:block; margin:0;}
.hr-scroll.open::after {display:none;}
.hr-scroll.open .scroll {white-space:normal;}

/* tooltip */
a.tool-tip {overflow:hidden; display:inline-block; width:32px; height:32px; vertical-align:middle;}
a.tool-tip .ico-tooltip {display:block; margin:0;}

.tooltip-contents {display:none;}
.tooltipster-sidetip.tool-style {margin:0; min-width:150px; max-width:100%;}
.tooltipster-sidetip.tool-style .tooltipster-box {margin:0; font-size:15px; border:2px solid #44465b; border-radius:12px; background-color:#fff; box-shadow: 20px 20px 20px 0 rgba(0, 0, 0, 0.1);}
.tooltipster-sidetip.tool-style .tooltipster-box .tooltipster-content {overflow:hidden; position:relative; padding:0; border-radius:12px 12px 0 0;}
.tooltipster-sidetip.tool-style .tooltipster-arrow {display:none;}
.tooltipster-sidetip.tool-style .tooltipster-content {line-height:1.53;}
.tooltipster-sidetip.tool-style .tooltipster-content .tit-tool {display:block; margin:0; padding:10px 50px 10px 10px; color:#333; line-height:1.3; font-weight:500; background-color:#f6f6f6; }
.tooltipster-sidetip.tool-style .tooltipster-content .cont-tool {display:block; padding:10px 50px 10px 10px; color:#555;}
.tooltipster-sidetip.tool-style .tooltipster-content .tit-tool + .cont-tool {padding:10px;} 
.tooltipster-sidetip.tool-style .tooltipster-content .tooltip-close {overflow:hidden; display:block; position:absolute; right:10px; top:8px; width:24px; height:24px; margin:0; padding:0; font-size:1px; text-indent:-9999px; background:url('../images/common/btn/btn-tooltip-close.png') no-repeat center; background-size:24px;}


/* layout
========================================================================== */
.skip {overflow:hidden; position:fixed; left:-99999px; top:-99999px; z-index:1600; width:1px; height:1px; font-size:1px; line-height:0.1; text-indent:-99999px}
.skip.on {left:0; top:0; width:100%; height:50px; font-size:14px; text-indent:0; text-align:center; background-color:#44465b;}
.skip.on a {display:inline-block; margin:8px 10px 0 10px; padding:0 10px; color:#fff; line-height:30px;}
.skip.on a:hover,
.skip.on a:focus {text-decoration:none; outline-color:#fff;}

/* header */
#header {position:fixed; left:0; top:0; z-index:100; width:100%; height:60px; border-bottom:1px solid #e1e3e6; background-color:#fff;}
#header > .wrap {position:relative; display:flex; flex-wrap:nowrap; justify-content:flex-start; align-items:center; width:100%; height:100%; padding:0 94px 0 20px;}
#header h1.logo {width:100%; height:100%;}
#header h1.logo.hidden {width:1px; height:1px;}
#header h1.logo a {display:inline-flex; align-items:center; height:100%;}
#header h1.logo a em {flex-shrink:0; display:block; overflow:hidden; width:34px; height:34px; margin:0; padding:0; border:0; text-indent:-9999px; background:url('../images/common/layout/logo-header.png') no-repeat 0 0; background-size:34px;}
#header h1.logo a strong {display:block; margin-left:8px; font-size:18px; line-height:1.1;}
#header .btn-back {flex-shrink:0; display:block; width:24px; height:24px; margin:0 0 0 -10px;}
#header .btn-back .iconset {display:block; margin:0;}
#header h2.title-2 {display:block; margin:0 10px 0 0; font-size:17px; line-height:1.18; font-weight:500;}

/* header util */
#header .header-util {position:absolute; right:20px; top:14px;}
#header .header-util .btn {display:inline-flex; justify-content:center; align-items:center; width:32px; height:32px; margin:0 0 0 10px; padding:0; border-radius:12px; box-shadow:0 4px 20px 0 rgba(0, 0, 0, 0.1); background-color:#fff; vertical-align:middle;}
#header .header-util .btn:first-child {margin-left:0;}
#header .header-util .btn .iconset {display:block; margin:0 auto;}

.all-menu {visibility:hidden; overflow:hidden; overflow-y:auto; position:fixed; right:-100vw; top:0; z-index:9999; width:100vw; height:100%; transition:right .3s ease-in-out, visibility .3s; background:#fff;}
.all-menu.on {visibility:visible; right:0;}

.all-menu .wrap {padding-left:20px;}
.all-menu .logo {position:sticky; left:0; top:0; height:60px; margin-left:-20px; padding:13px 0 0 20px; background-color:#fff;}
.all-menu .logo::after {content:''; display:block; position:absolute; left:-20px; bottom:0; width:calc(100% + 40px); height:1px; background-color:#e1e3e6;}
.all-menu .logo a {display:flex; align-items:center;}
.all-menu .logo em {display:block; width:34px; height:34px; margin-right:8px; text-indent:-9999px; background:url('../images/common/layout/logo-header.png') no-repeat; background-size:34px;}
.all-menu .logo strong {font-size:18px; color:#222; line-height:1.11;}

.all-menu .box {width:calc(100% + 20px); min-height:187px; margin:0 0 0 -20px; padding:30px 20px 24px 20px; background-color:#fff6f2;}
.all-menu .box .txt {margin:0;}
.all-menu .box .user-menu {overflow:hidden; list-style-type:none; margin:14px 0 0; padding:0;}
.all-menu .box .user-menu a {display:inline-block; margin:10px 6px 0 0; height:34px; padding:4px 16px 6px; border-radius:17px; color:#fff; font-size:16px; background-color:#44465b;}
.all-menu .box .user-menu a:last-child {margin-right:0; border:1px solid #e1e3e6; color:#3a3a4a; background-color:#fff;}

.all-menu .btn-close-all-menu {position:fixed; top:14px; right:10px; width:32px; height:32px; text-indent:-9999px; background:url('../images/common/btn/btn-close-modal.png') no-repeat; background-size:32px;}

/* gnb */
#gnb ul {list-style-type:none; margin:0; padding:0;}
#gnb > ul > li {padding:48px 0; border-bottom:1px solid #e1e3e6;}
#gnb > ul > li:last-child {border-bottom:0;}
#gnb > ul > li p {margin:0 0 20px; font-size:22px; font-weight:700; color:#3a3a4a; line-height:1.36;}
#gnb .depth-2 li + li {margin-top:16px;}
#gnb .depth-3 {margin:22px 0 24px 5px;}
#gnb .depth-3 li {position:relative; padding-left:12px;}
#gnb .depth-3 li::before {content:''; display:block; position:absolute; left:0; top:12px; width:4px; height:4px; border-radius:4px; background-color:#44465b;}

/* container */
.container {padding-top:60px;}

#contents {padding:30px 20px 80px;}

.hr-full {margin:0 -20px;}

#footer {padding:50px 0; border-top:1px solid #e1e3e6; font-size:12px; color:#717171; letter-spacing:-0.5px;}
#footer p {margin:0; padding:0;}
#footer .footer-link ul {display:flex; flex-wrap:wrap; justify-content:center; align-items:center; list-style-type:none; margin:0; padding:0; }
#footer .footer-link ul li {position:relative; margin:0 3px; padding:0 3px;}
#footer .footer-link ul li:before {content:''; display:block; position:absolute; left:-3px; top:50%; width:1px; height:10px; margin:-4px 0 0 0; background-color:#bec2c8;}
#footer .footer-link ul li:first-child {margin-left:0; padding-left:0;}
#footer .footer-link ul li:first-child:before {display:none;}
#footer .footer-link ul li:last-child {margin-right:0; padding-right:0;}
#footer .footer-link ul li a {display:inline-block; color:#555;}
#footer .footer-link ul li.privacy a {color:#3a3a4a; font-weight:500;}
#footer .family-site {padding:20px;}
#footer .family-site .site {position:relative; margin:0; font-size:13px;}
#footer .family-site .site + .site {margin-top:10px;}
#footer .family-site .site:before,
#footer .family-site .site:after {content:''; display:block;}
#footer .family-site .site:after {clear:both;}
#footer .family-site .site .btn-toggle-family {overflow:hidden; display:block; float:left; position:relative; width:calc(100% - 65px); height:40px; margin:0; padding:0 32px 0 16px; text-align:left; border-radius:8px 0 0 8px; border:1px solid #ccc; color:#555; white-space:nowrap; text-overflow:ellipsis; background-color:#fff;}
#footer .family-site .site .btn-toggle-family:after {content:''; display:block; position:absolute; right:16px; top:12px; width:16px; height:16px; background:#fff url('../images/common/bg/bg-select-arr.png') no-repeat 0 0; background-size:16px;}
#footer .family-site .site .site-move {overflow:hidden; display:block; float:left; position:relative; z-index:2; width:65px; height:40px; line-height:38px; text-align:center; text-decoration:none; border:1px solid #ccc; border-left:0; color:#555; border-radius:0 8px 8px 0; cursor:default;}
#footer .family-site .site .list {display:none; overflow:hidden; position:absolute; left:0; bottom:50px; z-index:3; width:100%; border-radius:8px; border:1px solid #44465b; background-color:#fff;}
#footer .family-site .site .list ul {overflow-y:auto; list-style-type:none; max-height:300px; margin:0; padding:22px 20px;}
#footer .family-site .site .list ul li {margin:0; padding:0;}
#footer .family-site .site .list ul li a {display:block; padding:8px 0; font-size:14px; color:#555; line-height:1.25;}
#footer .family-site .site .list ul li a:hover {color:#f6312b;}
#footer .family-site .site.on .list {display:block;}
#footer .family-site .site.on .btn-toggle-family:after {transform:rotate(180deg);}
#footer .address {margin:0; padding:0 20px; text-align:center; line-height:1.3;}
#footer .address span {display:inline;}
#footer .tel {padding:0 20px; text-align:center; line-height:1.3;}
#footer .tel span {display:inline;}
#footer .copy {margin:5px 0 0 0; padding:0 20px; font-family:'Roboto', sans-serif; text-align:center; line-height:1.3;}
#footer .copy span {display:inline;}

@media (max-width: 359px) {
  #footer .address span {display:block;}
  #footer .tel span {display:block;}
  #footer .copy span {display:block;}
}
