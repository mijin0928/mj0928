<script>
$(function(){
	$('#guideGnb a').on('click', function(e){
		e.preventDefault();
		
		var _href = $(this).attr('href');
		
		$(this).parent('li').siblings('li').removeClass('on').find('ul').slideUp(50).find('li').removeClass('on');
		
		
		if( $(this).parent('li').hasClass('has') ){
			$(this).parent('li').toggleClass('on').find('ul').slideToggle(50);
		}
		else {
			$(this).closest('li').addClass('on').siblings('li').removeClass('on');
			$(this).closest('li').siblings('li.has').find('ul').slideUp(50);
			
			$('.iframe').attr('src', _href);
		}
	});
});
</script>
