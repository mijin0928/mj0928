/* 개요 */
.box-procedure {padding:40px; border:1px solid #e1e3e6; border-radius:12px;}
.list-procedure {list-style-type:none; margin:0; padding:0; text-align:center;}
.list-procedure li {position:relative; margin-top:117px;}
.list-procedure li::before {content:''; position:absolute; top:calc(100% - -35px); left:calc(50% - 16px); display:block; width:32px;height:32px; background-image:url('../images/common/ico/ico-procedure-arr.png'); background-size:100%;}
.list-procedure li:first-child {margin-top:0;}
.list-procedure li:last-child::before {display:none;}
.list-procedure .icobox {width:160px; height:160px; margin:0 auto; padding:34px; border-radius:100%; background-color:#f6f6f6;}
.list-procedure .icobox.bg-green {background-color:#e4f2e4;}
.list-procedure p.txt {margin:20px 0 0 0;}

