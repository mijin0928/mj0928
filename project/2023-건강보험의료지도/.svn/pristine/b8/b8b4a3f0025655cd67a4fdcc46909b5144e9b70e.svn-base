/* 온라인 설문조사 상세 */
.board-form .wrap {margin-top:24px;}
.board-form .wrap + .wrap {margin-top:40px;}
.board-form .wrap .tit {margin:0 0 16px; font-weight:500; line-height:1.18; color:#3a3a4a;}
.line {display:block; width:100%; height:1px; background-color:#e2e3e7; margin-top:48px;}

/* 온라인 설문조사 상세(종료) */
.box-survey-result {padding:24px 20px; border:1px solid #e2e3e7; border-radius:12px; background-color:#fff6f2;}
.box-survey-result .list-dot li + li {margin-top:9px;}
.button.navy.open-modal {width:100%; margin-top:16px;}

/* layer-modal */
.layer-modal .wrap {padding:0 20px;}
.layer-modal .wrap .tit-area .tit {font-weight:500; color:#222; line-height:1.18;}
.layer-modal .wrap > .title-3 {margin:90px 0 20px;}
.layer-modal .box-survey-result.v1 {margin:20px 0 40px 0;}
.layer-modal .box-survey-result-list {display:block; margin:20px 0 0 0;}
.layer-modal .box-survey-result-list p {margin:0; padding:0;}
.layer-modal .box-survey-result-list .list {list-style-type:none; margin:0; padding:0;}
.layer-modal .box-survey-result-list .list li {position:relative; padding:24px 0; border-bottom:1px solid #e1e3e6;}
.layer-modal .box-survey-result-list .list li:first-child {padding-top:0;}
.layer-modal .box-survey-result-list .list li:last-child {border:0;}
.layer-modal .box-survey-result-list .list li .tit {display:flex; margin:0 0 24px;}
.layer-modal .box-survey-result-list .list li .tagset.navy.small {margin:2px 12px 0 0;} 


.layer-modal .box-survey-result-list .survey-graph dl {margin:0;}
.layer-modal .box-survey-result-list .survey-graph dt {line-height:1.65; font-weight:500;}
.layer-modal .box-survey-result-list .survey-graph dt em {position:relative;padding-left:17px; font-weight:400; word-break:keep-all;}
.layer-modal .box-survey-result-list .survey-graph dt em::after {content:''; display:block; position:absolute; left:8px; top:7px; width:1px; height:14px; background-color:#e1e3e6;}
.layer-modal .box-survey-result-list .survey-graph dd {margin:0;}
.layer-modal .box-survey-result-list .survey-graph dd span.bar {position:relative; display:inline-block; width:auto; height:16px; border-radius:0 8px 8px 0; background:#67caa8;}
