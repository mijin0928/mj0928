@charset "utf-8";
/* 자주하는 질문 */
.faq-list {list-style-type:none; padding:0; margin:0; border-top:2px solid #44465b;}
.faq-list > li {margin:0; padding:0; border-bottom:1px solid #e2e3e7;}
.faq-list > li .tit-area .tit {display:block; position:relative; width:100%; margin:0; padding:24px 44px 24px 36px; border:0; line-height:1.5; color:#3a3a4a; font-weight:500; text-align:left; background-color:#fff;}
.faq-list > li .tit-area .tit .ico-faq-q {position:absolute; left:0; top:24px;}
.faq-list > li .tit-area .tit .ico-toggle-arr {position:absolute; right:0; top:24px;}
.faq-list > li .cont-area {display:none; position:relative; padding:52px 20px 29px; font-size:16px; line-height:1.5; border-top:1px solid #e2e3e7; background-color:#fff6f2;}
.faq-list > li .cont-area .ico-faq-a {position:absolute; left:20px; top:20px;}


/* 온라인 설문조사 목록 */
.list-survey p {margin:0; padding:0;}
.list-survey ul {list-style-type:none; margin:0; padding:0;}
.list-survey ul li {margin:16px 0 0; padding:0;}
.list-survey ul li .box {display:block; position:relative; height:243px; padding:30px 30px 122px; border-radius:12px;}
.list-survey ul li .box .img {position:absolute; right:30px; bottom:20px; width:92px; height:92px; font-size:0; line-height:0;}
.list-survey ul li .box .img img {width:100%;}
.list-survey ul li .box .tit {overflow:hidden; height:60px; font-size:22px; color:#3a3a4a; font-weight:700; line-height:1.3; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical;}
.list-survey ul li .box .period {margin-top:10px; font-size:15px; line-height:1.2; letter-spacing:0; font-family:'Roboto', sans-serif;}
.list-survey ul li .box .count {position:absolute; left:30px; bottom:64px; margin:0; font-size:15px; line-height:1.2; letter-spacing:0; font-family:'Roboto', sans-serif;}
.list-survey ul li .box .count .iconset {margin-right:12px;}
.list-survey ul li:nth-child(3n) .box {background-color:#f4f5f7;}
.list-survey ul li:nth-child(3n - 1) .box {background-color:#e5fbf2;}
.list-survey ul li:nth-child(3n - 2) .box {background-color:#ffeae0;}
