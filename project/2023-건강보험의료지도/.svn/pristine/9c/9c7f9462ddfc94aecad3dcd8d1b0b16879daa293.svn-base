.flick-wrap {position:absolute; z-index:1; width:100%; height:100%; left:0; top:0; background-color:rgba(0, 0, 0, 0.3);}
.flick-wrap .flick-area {width:100%; height:100%; text-align:center;}
.flick-wrap .flick-area span {display:inline-block; position:absolute; left:50%; top:50%; z-index:2; transform:translate(-50%, -50%); width:90%; padding:10px; color:#fff; font-size:14px; text-align:center; border-radius:12px; background-color:#44465c;}

@media (max-width: 359px) {	
	.list-terms li .cont-area .box-gray-white.icon {padding:74px 20px 24px;}
	.list-terms li .cont-area .box-gray-white.icon .iconset {left:20px; top:24px; transform:translate(0);}
}
































































































/* 슬비 */

/* tab-list-line */
.tab-list-line {overflow:hidden; position:relative; margin:0 -20px 24px; padding:0 20px;}
.tab-list-line .scroll {overflow:hidden; overflow-x:scroll; white-space:nowrap; -ms-overflow-style:-ms-autohiding-scrollbar; scrollbar-width:none;}
.tab-list-line .scroll::-webkit-scrollbar {display:none;}
.tab-list-line .scroll .tab-list {display:inline-block; margin-bottom:0; vertical-align:top;}
.tab-list-line .list {list-style-type:none; margin:0; padding:0;}
.tab-list-line .list:after {content:''; position:absolute; bottom:0; left:0; display:block; width:1000px; height:1px; background-color:#e1e3e6;}
.tab-list-line .list > li {display:inline-block; padding:0 0 0 20px; vertical-align:top;}
.tab-list-line .list > li > a,
.tab-list-line .list > li > button {position:relative; overflow:hidden; display:block; padding:0; line-height:48px; font-size:16px; color:#717171; background-color:#fff;}
.tab-list-line .list > li > a:hover,
.tab-list-line .list > li > button:hover,
.tab-list-line .list > li.on > a,
.tab-list-line .list > li.on > button {font-weight:500; color:#3a3a4a;}
.tab-list-line .list > li.on > a:after,
.tab-list-line .list > li.on > button:after {content:''; position:absolute; bottom:0; left:0; display:block; width:100%; height:3px; background-color:#ee0700; z-index:1;}
.tab-list-line .list > li.on > a:focus,
.tab-list-line .list > li.on > button:focus {outline-color:#f6312b;}
.tab-list-line .list > li:first-child {padding:0;}











/* 로그인, 회원가입
========================================================================== */
/* 로그인 */
.login-wrap {}
.login-wrap .txt-area {position:relative;}
.login-wrap .txt-area:after {content:''; display:block; position:absolute; top:0; right:-20px; width:150px; height:140px; background:url('../images/contents/img-login.png') no-repeat; background-size:100%; background-position:0 0; z-index:-1;}
.login-wrap .txt-area .tit {padding:14px 0 0; margin:0; font-size:22px; font-weight:700; line-height:1.36;}
.login-wrap .txt-area .txt-page {margin:20px 0 0 0; font-size:17px; line-height:normal;}
.login-wrap .login-area {margin:24px 0 0;}
.login-wrap .login-area .input-wrap .txt-alert {margin:12px 0 0 0;}
.login-wrap .login-area .button {display:block; width:100%; height:56px; margin-top:20px; text-align:center;}
.login-wrap .login-area .user-menu {display:flex; justify-content:center; list-style-type:none; margin:20px 0 24px; padding:0;}
.login-wrap .login-area .user-menu li {position:relative; margin:0 0 0 10px; padding:0 0 0 10px;}
.login-wrap .login-area .user-menu li::before {content:''; display:block; position:absolute; left:0; top:8px; width:1px; height:14px; background-color:#e1e3e6;}
.login-wrap .login-area .user-menu li:first-child {margin:0; padding:0;}
.login-wrap .login-area .user-menu li:first-child::before {display:none;}
.login-wrap .login-area .list-dot {padding:24px 20px; margin:48px 0 0; background:#f7f9fa; border-radius:12px;}
.login-wrap .login-area .list-dot li {line-height:1.63;}

/* 아이디찾기 */
.find-id-pwd {}
.find-id-pwd .req-info {margin:0 0 20px; color:#717171; text-align:right; font-size:15px; line-height:1.1;}
.find-id-pwd .list-form {list-style-type:none; margin:0; padding:24px 0 40px; border-width:2px 0 1px; border-style:solid; border-color:#44465b transparent #e2e3e7;}
.find-id-pwd .list-form li {margin:20px 0 0 0; padding:0;}
.find-id-pwd .list-form li:first-child {margin-top:0;}
.find-id-pwd .list-form .label {margin:0 0 16px; color:#3a3a4a; font-weight:500;}
.find-id-pwd .box-right {margin:48px 0 0 0;}
.find-id-pwd .box-right .button {display:block; width:100%;}

.layer-modal .wrap-privacy {padding:22px 0; margin-top:24px; text-align:center; background-color:#fff6f2; border:solid 1px #e2e3e7; border-radius:12px;}
.layer-modal .wrap-privacy > .txt {margin:0; color:#3a3a4a; font-size:24px; font-weight:700;}

/* 비밀번호 찾기 */
.wrap-limit {position:relative; width:100%;}
.wrap-limit .input-text {padding-right:94px;}
.wrap-limit .time-limit {position:absolute; right:0; top:0; width:74px; height:100%; margin:0; font-family:'Roboto', sans-serif; font-size:14px; line-height:51px;}

.box-bg-gray {padding:24px 20px; margin-top:48px; border-radius:12px; background-color:#f7f9fa;}
.box-bg-gray .list-dot li {margin-top:9px; padding-left:12px; color:#717171; font-size:17px; line-height:1.7;}
.box-bg-gray .list-dot li:first-child {margin-top:0;}



/* 마이페이지 */
.mypage-main .txt-area {margin:0 0 40px; color:#555; font-size:16px;}
.mypage-main .txt-area .name {display:block; color:#3a3a4a; font-size:22px; font-weight:500;}
.mypage-main .link-area {margin:-20px 0 0 -20px;}
.mypage-main .link-area:after {content:''; display:block; clear:both;}
.mypage-main .link-area a {float:left; width:calc((100% - 40px) / 2); height:116px; margin:20px 0 0 20px; padding:20px; border-radius:12px; background-color:#e5fbf2; color:#3a3a4a; font-size:16px; font-weight:500; background-position:top 60px right 20px; background-size:36px; background-repeat:no-repeat;}
.mypage-main .link-area a:nth-child(1) {background-color:#ffeae0; background-image:url('../images/common/ico/ico-mypage01.png');}
.mypage-main .link-area a:nth-child(2) {background-image:url('../images/common/ico/ico-mypage02.png'); background-size:53px; background-position:top 57px right 17px;}
.mypage-main .link-area a:nth-child(4) {background-color:#ffeae0; background-image:url('../images/common/ico/ico-mypage03.png');}
.mypage-main .link-area a .num {display:block; margin:20px 0 0; color:#2fa787; font-size:16px; font-weight:500; text-align:right; line-height:1;}
.mypage-main .link-area a .num strong {margin-right:4px; font-size:28px; font-family:Roboto, sans-serif; font-weight:500;}
.mypage-main .link-area a:hover {text-decoration:none;}
.mypage-main .status-area {margin:20px 0 0; padding:20px; background-color:#f4f5f7; border-radius:12px; }
.mypage-main .status-area .tit {font-size:16px; font-weight:500;}
.mypage-main .status-area a {display:flex; justify-content:space-between; position:relative; padding:0 0 0 10px; margin:8px 0 0; font-size:15px;}
.mypage-main .status-area a:first-child {margin:0;}
.mypage-main .status-area a .num {display:inline-block; margin:0; color:#2fa787; font-size:16px; font-weight:500; text-align:right; line-height:1;}
.mypage-main .status-area a .num strong {margin-right:4px; font-size:28px; font-weight:500; font-family:Roboto, sans-serif; font-weight:500;}
.mypage-main .status-area a::before {content:''; position:absolute; top:50%; left:0; transform:translateY(-50%); display:block; width:4px; height:4px; background-color:#44465b;}
.mypage-main .btn-secession {text-align:right;}
.mypage-main .btn-secession a {color:#717171; font-size:15px; text-decoration:underline;}

.box-profile {overflow:hidden; padding:40px 0 40px 100px; border-radius:12px; background:#fff6f2 url('../images/common/ico/ico-profile02.png') no-repeat 0 14px; background-size:116px;}
.box-profile .txt {position:relative; float:left; margin:0; padding:0 0 0 48px;}
.box-profile .txt::before {content:''; position:absolute; top:50%; left:24px; display:block; width:1px; height:34px; margin-top:-14px; background-color:#e9dcd7;}
.box-profile .txt:first-child {padding:0;}
.box-profile .txt:first-child:before {display:none;}
.box-profile .txt span {color:#999; font-size:14px;}
.box-profile .txt strong {display:block; color:#3a3a4a; font-size:17px; font-weight:500;;}

@media (max-width: 359px) {
    #footer .address span {display:block;}

    /* 로그인, 회원가입
    ========================================================================== */
    /* 로그인 */
    .login-wrap .txt-area .tit {font-size:20px;}
    .login-wrap .txt-area:after {right:-45px;}
    .login-wrap .login-area .user-menu li {margin:0 0 0 5px; padding:0 0 0 5px; font-size:16px; letter-spacing:-0.5px;}

    /* 마이페이지 */
    .mypage-main .status-area {padding:20px;}
    .mypage-main .status-area .tit {position:static; margin:0 0 15px; transform:translateY(0);}


}


