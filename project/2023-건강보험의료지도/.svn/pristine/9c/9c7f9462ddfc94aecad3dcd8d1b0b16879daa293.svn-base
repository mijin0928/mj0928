.list-procedure p.txt {margin:20px 0 0 0;}

/* 의료이용지표현황 */
/* 목록 */
.indicator-list p {margin:0; padding:0;}
.indicator-list ul {list-style-type:none; margin:0; padding:0;}
.indicator-list ul li {margin:16px 0 0 0; padding:0;}
