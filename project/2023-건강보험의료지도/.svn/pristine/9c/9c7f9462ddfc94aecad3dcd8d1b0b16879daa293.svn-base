.indicator-guide-list:after {content:''; display:block;}
.indicator-guide-list > li a {position:relative; display:block; width:100%; height:140px; padding:20px; margin:16px 0 0 0; border:1px solid rgba(255,255,255,0); background-color:#f4f5f7; border-radius:24px; transition:0.2s;}
