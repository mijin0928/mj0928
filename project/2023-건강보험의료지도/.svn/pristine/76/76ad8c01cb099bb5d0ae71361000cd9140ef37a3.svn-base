@charset "utf-8";
/* 서비스 소개
========================================================================== */
/* 의료이용지표 개요 */
.list-procedure {display:flex; justify-content:center; list-style-type:none; margin:0; padding:0;}
.list-procedure > li {position:relative; width:312px; text-align:center;}
.list-procedure > li::before {content:''; position:absolute; top:64px; left:-16px; display:block; width:32px; height:32px; background-image:url('../images/common/ico/ico-procedure-arr.png'); background-size:100%;}
.list-procedure > li:first-child::before {display:none;}
.list-procedure > li .ico {display:inline-block; width:160px; height:160px; padding:34px 0 0; margin:0; border-radius:50%; background-color:#f6f6f6;}
.list-procedure > li .ico.bg-green {background-color:#e4f2e4;}
.list-procedure > li .ico.bg-pink {background-color:#ffeae0;}
.list-procedure > li .txt {display:block; margin:10px 0 0;}


/* 의료이용지표
========================================================================== */


/* 이용가이드
========================================================================== */
/* 지표 이용 가이드 - 목록 */
.indicator-guide-list {list-style-type:none; margin:-40px 0 0 -40px; padding:0;}
.indicator-guide-list:after {content:''; display:block; clear:both}
.indicator-guide-list > li {position:relative; float:left; width:620px; height:220px; padding:30px 40px; margin:40px 0 0 40px; border:1px solid rgba(255,255,255,0); background-color:#f4f5f7; border-radius:24px; transition:0.2s;}
.indicator-guide-list > li:hover {border:1px solid #bec2c8;  box-shadow: 0 8px 24px 0 rgba(0, 0, 0, 0.08);}
.indicator-guide-list > li .tit {margin:0; padding:0; color:#3a3a4a; font-size:22px; font-weight:700; line-height:1.36;}
.indicator-guide-list > li .button {margin-top:40px;}
.indicator-guide-list > li .iconset {position:absolute; top:50px; right:60px;}

/* 오픈 API 목록 */
.api-list {list-style-type:none; margin:0; padding:0;}
.api-list > li {margin:0; padding:0;}
.api-list > li + li {margin-top:40px;}
.api-list > li p {margin:0;}
.api-list > li > a {display:block; min-height:178px; padding:30px 40px; text-align:left; border:1px solid #e2e3e7; border-radius:24px; transition:box-shadow 0.2s;}
.api-list > li > a:hover,
.api-list > li > a:focus {text-decoration:none; background-color:#fff6f2; border-color:#bec2c8; box-shadow:0 8px 24px 0 rgba(0,0,0,0.08);}
.api-list > li .tit {color:#3a3a4a; font-size:22px; font-weight:700; line-height:1.36; letter-spacing:-0.22px;}
.api-list > li .txt {line-height:1.25; letter-spacing:-0.16px;}
.api-list > li > a > .txt {overflow:hidden; margin:10px 0 20px; white-space:nowrap; text-overflow:ellipsis;}
.api-list > li .data {display:flex; justify-content:space-between;}
.api-list > li .data > .wrap-txt {width:calc(100% - 286px); flex-shrink:0;}
.api-list > li .data > .wrap-txt .txt {display:inline-block; position:relative; padding-left:12px; margin-left:8px; vertical-align: middle;}
.api-list > li .data > .wrap-txt .txt:after {content:''; position:absolute; left:0; top:5.5px; width:1px; height:14px; background-color:#bec2c8;}
.api-list > li .data > .wrap-txt .txt:first-child {margin:0; padding:0;}
.api-list > li .data > .wrap-txt .txt:first-child:after {display:none;}
.api-list > li .data > .tagset-group .tagset {padding:0 16px; margin-right:8px;}
.api-list > li .data > .tagset-group .tagset:last-child {margin-right:0;}

/* 오픈 API 상세보기 */
.api-view {border:1px solid #e2e3e7; border-radius:12px;}
.api-view p {margin:0; padding:0;}
.api-view .tit-area {position:relative; min-height:88px; padding:29px 180px 29px 40px; background-color:#fff6f2; border-radius:12px 12px 0 0; border-bottom:1px solid #e2e3e7;}
.api-view .tit-area > .tit {color:#3a3a4a; font-size:24px; font-weight:700; line-height:1.25; letter-spacing:-0.24px;}
.api-view .tit-area > .button {position:absolute; right:40px; top:50%; margin:-24px 0 0;}
.api-view .cont-area {padding:40px;}
.api-view .cont-area .list-dot li {display:flex; flex-wrap:nowrap; padding-left:12px; margin-top:20px;}
.api-view .cont-area .list-dot li:first-child {margin-top:0;}
.api-view .cont-area .list-dot li:before {top:9px; background-color:#44465b;}
.api-view .cont-area .list-dot .tit,
.api-view .cont-area .list-dot .txt {color:#3a3a4a; font-weight:500; line-height:1.25; letter-spacing:-0.16px; word-break:break-all;}
.api-view .cont-area .list-dot .tit {flex-shrink:0; width:59px; margin-right:42px;}
.api-view .cont-area .list-dot .txt {font-weight:400;}
.api-view .cont-area .add-file-area > .list {margin:-15px 0 0 0;}
.api-view .cont-area .data {margin-top:40px; padding:11px 30px; background-color:#f6f6f6; border-radius:8px;}
.api-view .cont-area .data > .txt {display:inline-block; position:relative; padding-right:12px; margin-right:8px; line-height:1.25; letter-spacing:-0.16px;}
.api-view .cont-area .data > .txt:after {content:''; position:absolute; right:0; top:5px; width:1px; height:14px; background-color:#bec2c8;}
.api-view .cont-area .data > .txt:last-of-type {margin:0; padding:0;}
.api-view .cont-area .data > .txt:last-of-type:after {display:none;}
.api-edit-view {overflow:hidden; padding:40px; color:#3a3a4a; border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7;}
.api-edit-view img {max-width:100% !important;}


/* 고객센터
========================================================================== */
/* 자주하는 질문 */
.faq-list {list-style-type:none; padding:0; margin:0; border-top:2px solid #44465b;}
.faq-list > li {margin:0; padding:0; border-bottom:1px solid #e2e3e7;}
.faq-list > li .tit-area .tit {display:block; position:relative; width:100%; margin:0; padding:30px 92px; border:0; font-size:18px; line-height:1.4; color:#3a3a4a; font-weight:500; text-align:left; background-color:#fff;}
.faq-list > li .tit-area .tit .ico-faq-q {position:absolute; left:40px; top:30px;}
.faq-list > li .tit-area .tit .ico-toggle-arr {position:absolute; right:30px; top:50%; margin-top:-16px;}
.faq-list > li .cont-area {display:none; position:relative; padding:30px 50px 30px 92px; font-size:16px; line-height:1.75; border-top:1px solid #e2e3e7; background-color:#fff6f2;}
.faq-list > li .cont-area .ico-faq-a {position:absolute; left:40px; top:30px;}

/* 온라인 설문조사 목록 */
.list-survey p {margin:0; padding:0;}
.list-survey ul {list-style-type:none; margin:0 0 0 -40px; padding:0;}
.list-survey ul:before,
.list-survey ul:after {content:''; display:block;}
.list-survey ul:after {clear:both;}
.list-survey ul li {float:left; width:calc(50% - 40px); margin:0 0 0 40px; padding:0;}
.list-survey ul li:nth-child(2) ~ li {margin-top:40px;}
.list-survey ul li .box {display:block; position:relative; width:100%; height:220px; padding:30px 240px 30px 40px; border-radius:24px; border:1px solid rgba(255,255,255,0); transition:box-shadow 0.2s;}
.list-survey ul li .box:hover {border:1px solid #bec2c8; text-decoration:none; box-shadow: 0 8px 24px 0 rgba(0, 0, 0, 0.08);}
.list-survey ul li .box .state {overflow:hidden; position:absolute; right:40px; top:30px; z-index:2; width:60px; height:31px; line-height:30px; border-radius:12px; text-align:center; color:#fff; background-color:#44465b;}
.list-survey ul li .box .img {position:absolute; right:60px; top:50px; width:120px; height:120px; font-size:0; line-height:0;}
.list-survey ul li .box .tit {overflow:hidden; height:60px; font-size:22px; color:#3a3a4a; font-weight:700;  line-height:1.3; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical;}
.list-survey ul li .box .period {margin-top:12px; line-height:1.1; letter-spacing:0; font-family:'Roboto', sans-serif;}
.list-survey ul li .box .count {position:absolute; left:40px; bottom:30px; margin:0; line-height:1.1; letter-spacing:0; font-family:'Roboto', sans-serif;}
.list-survey ul li .box .count .iconset {margin-right:6px;}
.list-survey ul li:nth-child(3n) .box {background-color:#f4f5f7;}
.list-survey ul li:nth-child(3n - 1) .box {background-color:#e5fbf2;}
.list-survey ul li:nth-child(3n - 2) .box {background-color:#ffeae0;}

/* 온라인 설문조사 보기 */
.tit-board-survey {margin:0; padding:0; font-size:32px; font-weight:700; text-align:center;}
.txt-board-survey {margin:0; padding:20px; font-size:16px; text-align:center;}

.box-survey-result {position:relative; min-height:88px; padding:30px 180px 30px 40px; border:1px solid #e2e3e7; border-radius:12px; background-color:#fff6f2;}
.box-survey-result.v1 {min-height:60px; padding:20px 0; text-align:center;}
.box-survey-result .list-dot {margin:0;}
.box-survey-result .list-dot li {display:inline-block; margin-left:40px; margin-top:0;}
.box-survey-result .list-dot li:first-child {margin-left:0;}
.box-survey-result .button {position:absolute; right:40px; top:50%; margin:-24px 0 0 0;}

.box-survey-result-list {display:block; margin:20px 0 0 0; padding:0 30px; border:1px solid #e2e3e7; border-radius:12px;}
.box-survey-result-list p {margin:0; padding:0;}
.box-survey-result-list .list {list-style-type:none; margin:0; padding:0;}
.box-survey-result-list .list > li {padding:30px 0; border-bottom:1px solid #e1e3e6;}
.box-survey-result-list .list > li:last-child {border:0;}
.box-survey-result-list .list > li .tit {position:relative; margin:0; padding:0 0 0 52px; font-size:16px; font-weight:500; color:#3a3a4a; line-height:1.5;}
.box-survey-result-list .list > li .tit .tagset {position:absolute; left:0; top:1px; width:42px; font-family:'Roboto',sans-serif;}
.box-survey-result-list .list > li .txt {font-size:15px; margin:20px 0 0; padding:0 0 0 56px;}

.box-survey-result-list .survey-graph {display:block; margin:30px 0 0; padding-left:54px;}
.box-survey-result-list .survey-graph dl {display:flex; flex-wrap:wrap; align-items:center; margin-bottom:12px; font-size:15px;}
.box-survey-result-list .survey-graph dl:last-child {margin-bottom:0;}
.box-survey-result-list .survey-graph dt {width:150px; padding-right:10px; color:#555; line-height:1.5;}
.box-survey-result-list .survey-graph dd {position:relative; margin:0; padding:5px 180px 5px 0; width:calc(100% - 150px);}
.box-survey-result-list .survey-graph dd span.bar {position:relative; display:inline-block; width:auto; height:16px; border-radius:0 16px 16px 0; background:#67caa8;}
.box-survey-result-list .survey-graph dd em {position:absolute; display:inline-block; margin-left:30px; line-height:18px; word-break: keep-all;}













.tab-list-full + .req-info {margin:40px 0 0;}

/* 마이페이지 */
.ico-receipt {width:24px; height:24px; margin-top:-2px; margin-right:6px; background-image:url('../images/common/ico/ico-receipt.png');} /* 접수 */
.ico-judge {width:24px; height:24px; margin-top:-2px; margin-right:6px; background-image:url('../images/common/ico/ico-judge.png');} /* 심사 */
.ico-companion {width:24px; height:24px; margin-top:-2px; margin-right:6px; background-image:url('../images/common/ico/ico-companion.png');} /* 반려 */
.ico-nonember {width:24px; height:24px; margin-top:-2px; margin-right:6px; background-image:url('../images/common/ico/ico-approval.png');} /* 승인 */

/* 게시판 목록 */
.board-list table thead tr th {height:55px; padding:10px; line-height:1.4666; text-align:center; color:#111; font-weight:500; border:1px solid #e2e3e7; background-color:#f4f5f7;}
.board-list table thead tr th:first-child {border-left:0;}
.board-list table thead tr th:last-child {border-right:0;}
.board-list table tr td {height:58px; padding:13px 20px; text-align:center; border:1px solid #e2e3e7;}
.board-list table tr td:first-child {border-left:0;}
.board-list table tr td:last-child {border-right:0;}
.board-list table thead tr:first-child th,
.board-list table tbody tr:first-child th,
.board-list table tbody tr:first-child td {border-top:0;}
.board-list table tr td.link {text-align:left;}
.board-list table tr td.link > a {display:inline-block; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; max-width:100%; vertical-align:middle;}

/* board-view02 */
.board-view02 {padding:30px 40px; border-top:2px solid #44465b; border-bottom:1px solid #e2e3e7;}
.board-view02 > ul {list-style-type:none; margin:0; padding:0;}
.board-view02 > ul > li {display:flex; padding:10px 0;}
.board-view02 > ul > li .tit {position:relative; min-width:150px; padding-left:12px; color:#3a3a4a; font-size:16px; font-weight:500;}
.board-view02 > ul > li .tit:before {content:''; position:absolute; top:13px; left:0; display:block; width:4px; height:4px; background-color:#44465b;}
.board-view02 > ul > li .txt {margin:0; width:440px;  word-break:keep-all;}
.board-view02 > ul > li .txt:only-of-type {width:100%;}