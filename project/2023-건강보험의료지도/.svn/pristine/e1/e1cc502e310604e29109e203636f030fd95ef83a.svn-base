.ico-privacy-mark-02 {width:40px; height:40px; background-image:url('../images/contents/ico-privacy-mark-02.png');}

.list-privacy {display:flex; flex-wrap:wrap; justify-content:center; list-style-type:none; margin:70px 0; padding:0;}
.list-privacy > li {margin:0 40px 0 0;}
.list-privacy > li:nth-child(5n),
.list-privacy > li:last-child {margin-right:0;}
.list-privacy > li:nth-child(5) ~ li {margin-top:40px;} 
.list-privacy > li a {display:block; width:224px; height:207px; padding:30px 0; text-align:center; border:1px solid #ddd; border-radius:24px; transition:box-shadow 0.3s;}
.list-privacy > li a:hover,
.list-privacy > li a:focus {text-decoration:none; background-color:#fff6f2; border:1px solid #bec2c8; box-shadow:0 8px 24px 0 rgba(0,0,0,0.08);}
.list-privacy > li .tit {margin:15px 0 0; color:#333; font-weight:700; line-height:1.8;}

.cont-tool.privacy .list-dot ol,
.cont-tool.privacy .list-dot ol > li {list-style-type:none; margin:0; padding:0;}
.cont-tool.privacy .list-dot ol,
.cont-tool.privacy .list-dot ul {margin-top:10px;}

.list-privacy + .board-util > .title-3 {margin:0;}
.list-terms {list-style-type:none; margin:0; padding:0; border-top:2px solid #44465b;}
.list-terms > li {margin:0;}
.list-terms li .tit-area .tit {position:relative; width:100%; min-height:92px; padding:33px 254px 32px 102px; color:#3a3a4a; font-size:18px; font-weight:500; text-align:left; line-height:1.1; background-color:#fff; border-bottom:1px solid #e2e3e7;}
.list-terms li .tit-area .tit .num {position:absolute; left:46px; top:50%; transform:translateY(-50%); margin:0; font-family:'Roboto', sans-serif;} 
.list-terms li .tit-area .tit .ico-toggle-arr,
.list-terms li .tit-area .tit .wrap-ico-privacy {position:absolute; top:50%; transform:translateY(-50%);}
.list-terms li .tit-area .tit .ico-toggle-arr {right:30px;}
.list-terms li .tit-area .tit .wrap-ico-privacy {overflow:hidden; right:92px; width:124px; height:48px;}
.list-terms li .tit-area .tit .wrap-ico-privacy .iconset {float:left; margin-right:2px;}
.list-terms li .tit-area .tit .wrap-ico-privacy .iconset:last-child {margin-right:0;}
.list-terms li .cont-area {display:none; padding:30px 102px; background-color:#fff6f2; border-bottom:1px solid #e2e3e7;}
.list-terms li .cont-area .txt {margin:10px 0 0;}
.list-terms li .cont-area .txt:first-child {margin:0;}
.list-terms li .cont-area .txt + ol {margin-top:10px;}
.list-terms li .cont-area ol {list-style-type:none; padding:0; margin:0;}
.list-terms li .cont-area ol > li {margin:0; word-break:keep-all;}
.list-terms li .cont-area > ol > li + li {margin-top:10px;}
.list-terms li .cont-area .list-kor > li {position:relative; padding-left:23px; word-break:keep-all;}
.list-terms li .cont-area .list-kor > li span {position:absolute; left:0; top:0;}
.list-terms li .cont-area li .button {margin-left:10px;}
.list-terms li .cont-area .list-dash li:before {top:14px;}
.list-terms li .cont-area .box-gray-white {margin:15px 0 30px; padding:20px 40px; border:1px solid #e2e3e7; background-color:#fff; border-radius:12px;}
.list-terms li .cont-area > .box-gray-white:last-child,
.list-terms li .cont-area li:last-child > .box-gray-white {margin-bottom:0;}
.list-terms li .cont-area .box-gray-white.icon {position:relative; padding:32px 40px 32px 107px;}
.list-terms li .cont-area .box-gray-white.icon .iconset {position:absolute; left:40px; top:50%; transform:translateY(-50%);}
.list-terms li .cont-area .box-gray-white.icon .iconset + em {display:inline-block; margin-left:10px;}
.list-terms li .cont-area .box-gray-white > ol > li + li {margin-top:10px;}
.list-terms li .cont-area .box-gray-white .tit {margin:0 0 10px; font-weight:700;}
.list-terms li .cont-area .img {margin:10px auto; width:688px;}
.list-terms li .cont-area .img img {display:block;}
.list-terms li .cont-area .img + .txt-help {width:688px; margin:0 auto;}
.list-terms li .cont-area .table-data {margin-top:10px;}
.list-terms li .cont-area .table-data table thead tr th {text-align:center;}
.list-terms li .cont-area em {font-weight:700;}

/* list-dot 안에 버튼(정렬) */
.list-terms .cont-area .has-button > li {display:flex; align-items:center; position:static; padding-left:0;}
.list-terms .cont-area .has-button > li:before {display:none;}
.list-terms .cont-area .has-button > li > span {display:inline-block; position:relative; padding-left:10px;}
.list-terms .cont-area .has-button > li > span:before {content:''; position:absolute; top:13px; left:0; width:4px; height:4px; background-color:#333; border-radius:2px;}
.list-terms .cont-area .has-button .button {flex-shrink:0;}
