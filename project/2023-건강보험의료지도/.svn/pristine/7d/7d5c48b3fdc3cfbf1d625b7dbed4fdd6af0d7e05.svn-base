/* 슬비 */

/* tab-list-line */

/* tab 가로 스크롤 */
.tab-list-line {overflow:hidden; position:relative; margin:0 -20px 24px;}
.tab-list-line::after {content:''; display:block; position: absolute; right:0; top:0; width:26px; height:34px; background-image: linear-gradient(to right, rgba(255, 255, 255, 0) 10%, rgba(255, 255, 255, 0.3) 30%, rgba(255, 255, 255, 1) 60%);}
.tab-list-line .scroll {overflow:hidden; overflow-x:scroll; white-space:nowrap; -ms-overflow-style:-ms-autohiding-scrollbar; scrollbar-width:none;}
.tab-list-line .scroll::-webkit-scrollbar {display:none;}
.tab-list-line .scroll .tab-list {display:inline-block; margin-bottom:0; vertical-align:top;}
.tab-list-line .list {list-style-type:none; margin:0; padding:0;}
.tab-list-line .list:after {content:''; position:absolute; bottom:0; left:0; display:block; width:1000px; height:1px; background-color:#e1e3e6;}
.tab-list-line .list > li {display:inline-block; padding:0 0 0 20px; vertical-align:top;}
.tab-list-line .list > li > a,
.tab-list-line .list > li > button {position:relative; overflow:hidden; display:block; line-height:48px; font-size:16px; color:#717171;}
.tab-list-line .list > li > a:hover,
.tab-list-line .list > li > button:hover,
.tab-list-line .list > li.on > a,
.tab-list-line .list > li.on > button {color:#3a3a4a;}
.tab-list-line .list > li.on > a:after,
.tab-list-line .list > li.on > button:after {content:''; position:absolute; bottom:0; left:0; display:block; width:100%; height:3px; background-color:#ee0700; z-index:1;}
.tab-list-line .list > li.on > a:focus,
.tab-list-line .list > li.on > button:focus {outline-color:#f6312b;}











