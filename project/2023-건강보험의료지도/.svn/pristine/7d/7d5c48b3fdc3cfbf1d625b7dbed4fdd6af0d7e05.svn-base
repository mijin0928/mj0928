.step-join {display:block; margin:0 0 48px 0; text-align:center;}
.step-join .wrap {overflow:hidden; display:inline-block; position:relative; text-align:center;}
.step-join .wrap::before {content:''; display:block; position:absolute; left:40px; right:40px; top:27px; z-index:0; height:2px; background-color:#bec2c8;}
.step-join .wrap .box {display:inline-block; margin:0; padding:0 20px 46px 20px; text-align:center;}
.step-join .wrap .box .num {display:block; position:relative; width:48px; height:48px; line-height:49px; border-radius:20px; font-size:20px; font-weight:700; font-family:Roboto, sans-serif; color:#fff; text-align:center; background-color:#44465b;}
.step-join .wrap .box .tit {display:none; position:absolute; left:0; bottom:0; width:100%; margin:0; padding:0; font-size:18px; font-weight:500; color:#3a3a4a; line-height:1.1; text-align:center;}
.step-join .wrap .box.done .num,
.step-join .wrap .box.active .num {text-indent:-9999px; background:#3bc897 url('../images/common/ico/ico-check.png') no-repeat center; background-size:24px;}
.step-join .wrap .box.active .tit {display:block;}

@media (max-width: 359px) {
	.step-join .wrap .box {padding:0 10px 46px 10px;}
}



